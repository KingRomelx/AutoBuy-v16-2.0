for i, macro in ipairs(EmoteMacros) do
    local btn = Instance.new("TextButton")
    btn.Text = macro.btnLabel or macro.text
    btn.Font = Enum.Font.FredokaOne
    btn.TextSize = 21
    btn.TextColor3 = Color3.fromRGB(242,242,255)
    btn.BackgroundColor3 = Color3.fromRGB(39,60,130)
    btn.Size = UDim2.new(1, -40, 0, macroBtnHeight)
    btn.Position = UDim2.new(0, 22, 0, 52 + (i-1)*(macroBtnHeight + macroSpace))
    btn.AutoButtonColor = true
    btn.Parent = EmotesPanel

    local function sendEmote()
        local msg = macro.text
        local event = game:GetService("ReplicatedStorage"):FindFirstChild("DefaultChatSystemChatEvents")
        if event and event:FindFirstChild("SayMessageRequest") then
            event.SayMessageRequest:FireServer(msg, "All")
        end
    end
    btn.MouseButton1Click:Connect(sendEmote)
    if macro.keycode then
        KeycodeToMacro[macro.keycode.Value] = sendEmote
    end
end

-- Keybinds: only active if tab visible
game:GetService("UserInputService").InputBegan:Connect(function(input, gp)
    if not EmotesPanel.Visible or gp then return end
    local fn = KeycodeToMacro[input.KeyCode.Value]
    if fn then
        fn()
    end
end)

-- Tab BUTTON logic (standard tab switching pattern)

local PlayerGui = player:WaitForChild("PlayerGui")

-- Main Suite Frame
local suiteFrame = Instance.new("Frame")
suiteFrame.Name = "JarvisSuite"
suiteFrame.Size = UDim2.new(0, 1030, 0, 790) -- Larger new size!
suiteFrame.Position = UDim2.new(0.5, -515, 0.5, -395)
suiteFrame.BackgroundColor3 = Color3.fromRGB(29,36,54)
suiteFrame.BorderSizePixel = 0
suiteFrame.AnchorPoint = Vector2.new(0.5,0.5)
suiteFrame.Visible = true
suiteFrame.Parent = PlayerGui

-- Suite Title
local titleBar = Instance.new("TextLabel")
titleBar.Name = "SuiteTitle"
titleBar.Text = "JARVIS SNEAKER SUITE"
titleBar.Font = Enum.Font.FredokaOne
titleBar.TextSize = 38
titleBar.TextColor3 = Color3.fromRGB(84,255,235)
titleBar.BackgroundTransparency = 1
titleBar.Size = UDim2.new(1,0,0,56)
titleBar.Position = UDim2.new(0,0,0,0)
titleBar.Parent = suiteFrame


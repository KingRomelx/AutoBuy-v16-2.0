    pan.Position = UDim2.new(0.03,0,0,tabY)
    pan.Size = UDim2.new(0.94,0,1,-(tabY+18))
    pan.BackgroundTransparency = 0.42
    pan.BackgroundColor3 = Color3.fromRGB(210, 235, 245)
    pan.ZIndex = 1
    pan.Visible = false
    local smC = Instance.new("UICorner", pan)
    smC.CornerRadius = UDim.new(0,13)
    panels[i] = pan

    -- Glass blur and shine for style
    local smGlass = Instance.new("ImageLabel")
    smGlass.Name = "GlassBlur"
    smGlass.BackgroundTransparency = 1
    smGlass.ImageTransparency = 0.56
    smGlass.Size = UDim2.new(1,0,1,0)
    smGlass.Position = UDim2.new(0,0,0,0)
    smGlass.ZIndex = 2
    smGlass.Image = "rbxassetid://8731281434"
    smGlass.Parent = pan

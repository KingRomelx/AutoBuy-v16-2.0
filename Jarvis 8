    smShine.BackgroundTransparency = 0.68
    smShine.BackgroundColor3 = Color3.fromRGB(255,255,255)
    smShine.Size = UDim2.new(1,0,0,22)
    smShine.Position = UDim2.new(0,0,0,0)
    smShine.ZIndex = 3
    smShine.Parent = pan

    btn.MouseButton1Click:Connect(function()
        for j,b in ipairs(tabButtons) do
            b.BackgroundColor3 = Color3.fromRGB(24,24,38)
            b.TextColor3 = Color3.fromRGB(144,200,255)
            panels[j].Visible = false
        end
        btn.BackgroundColor3 = Color3.fromRGB(0,250,255)
        btn.TextColor3 = Color3.fromRGB(24,24,29)
        pan.Visible = true
    end)
end
--[[
================================================================================
BLOCK 3.8: SNEAKER MONITOR TAB GUI & LOGIC (JARVIS SM TAB, ACCIDENTAL REPUT PREVENTION)
================================================================================
]]

local pan = panels[#panels]

local RED   = Color3.fromRGB(255,60,60)
local GREEN = Color3.fromRGB(63,220,120)
local GRAY  = Color3.fromRGB(44,44,44)
local player = game:GetService("Players").LocalPlayer
local myInfo = player:WaitForChild("PlayerInfo")
local replicated = game:GetService("ReplicatedStorage")
local remote = replicated:WaitForChild("RemoteEvents"):WaitForChild("ChangeDisplaySneaker")
local UIS = game:GetService("UserInputService")
local enableSMNotifs = true

local title = Instance.new("TextLabel")
title.Text = "Sneaker Monitor"
title.Font = Enum.Font.FredokaOne
title.TextColor3 = Color3.fromRGB(100,255,215)
title.TextSize = 25
title.BackgroundTransparency = 1
title.Size = UDim2.new(1,0,0,42)
title.Position = UDim2.new(0,0,0,2)
title.Parent = pan

local smStatusLabels = {}
for i=1,3 do
    local lab = Instance.new("TextLabel")
    lab.Font = Enum.Font.Code
    lab.TextSize = 18
    lab.BackgroundTransparency = 0.2
    lab.BackgroundColor3 = Color3.fromRGB(33,33,44)
    lab.BorderSizePixel = 0
    lab.TextColor3 = Color3.fromRGB(222,222,222)
    lab.TextXAlignment = Enum.TextXAlignment.Left
    lab.Size = UDim2.new(0.95,0,0,44)
    lab.Position = UDim2.new(0.025,0,0,50 + (i-1)*52)
    lab.Name = "Status"..i
    lab.Parent = pan
    smStatusLabels[i] = lab
end

local notifToggle = Instance.new("TextButton")
notifToggle.Size = UDim2.new(0.45, 0, 0, 35)
notifToggle.Position = UDim2.new(0.06, 0, 0, 212)
notifToggle.Text = "Notifs: ON"
notifToggle.BackgroundColor3 = Color3.fromRGB(60,180,100)
notifToggle.TextColor3 = Color3.new(1,1,1)
notifToggle.Font = Enum.Font.Code
notifToggle.TextSize = 17
notifToggle.Parent = pan

notifToggle.MouseButton1Click:Connect(function()
    enableSMNotifs = not enableSMNotifs
    notifToggle.Text = "Notifs: " .. (enableSMNotifs and "ON" or "OFF")
    notifToggle.BackgroundColor3 = enableSMNotifs and Color3.fromRGB(60,180,100) or Color3.fromRGB(80,80,90)
end)

local removeAllBtn = Instance.new("TextButton")
removeAllBtn.Size = UDim2.new(0.42, 0, 0, 35)
removeAllBtn.Position = UDim2.new(0.52, 4, 0, 212)
removeAllBtn.Text = "REMOVE ALL (Q)"
removeAllBtn.BackgroundColor3 = Color3.fromRGB(220,54,54)
removeAllBtn.TextColor3 = Color3.new(1,1,1)
removeAllBtn.Font = Enum.Font.FredokaOne
removeAllBtn.TextSize = 18
removeAllBtn.Parent = pan

local function removeAll()
    for i=1,3 do
        local args = {"", "DisplaySneaker"..i, 0}
        remote:FireServer(unpack(args))
    end
end
removeAllBtn.MouseButton1Click:Connect(removeAll)
UIS.InputBegan:Connect(function(input, gameProcessed)
    if pan.Visible and not gameProcessed and input.KeyCode == Enum.KeyCode.Q then
        removeAll()
    end
end)


--[[
================================================================================
BLOCK 2: JARVIS TAB PANELS & UNIVERSAL TAB SWITCHING LOGIC
================================================================================
]]

-- Panels: one for each tab
local TabPanels = {}

local function makePanel(tabName)
    local p = Instance.new("Frame")
    p.Name = tabName:gsub(" ", "").."Panel"
    p.Size = UDim2.new(1,0,1,0)
    p.Position = UDim2.new(0,0,0,0)
    p.BackgroundColor3 = Color3.fromRGB(33,37,48)
    p.BorderSizePixel = 0
    p.Visible = false
    p.Parent = TabContents
    TabPanels[tabName] = p
    return p
end

-- Create empty panels for all tabs (you'll fill in each with its corresponding feature content)
for _, tabName in ipairs({"Autobuy","Blacklist","Search","Log","Sales Log","Stats","Sneaker Monitor","Emotes"}) do
    makePanel(tabName)
end

-- Example (add module logic/content for each tab inside its panel)
do
    local label = Instance.new("TextLabel")
    label.Text = "Autobuy coming soon!"
    label.Font = Enum.Font.Code
    label.TextSize = 28
    label.TextColor3 = Color3.fromRGB(77,233,150)
    label.BackgroundTransparency = 1
    label.Size = UDim2.new(1,0,0,44)
    label.Position = UDim2.new(0,0,0,72)
    label.Parent = TabPanels["Autobuy"]
end
-- (repeat above template for Blacklist/Log etc, or fill with your real feature UIs)

-- Tab switching logic
local function showPanel(tabName)
    for key,panel in pairs(TabPanels) do
        panel.Visible = (key == tabName)
    end
    -- Tab active button highlight (optional, simple blue underline)
    for k,btn in pairs(TabButtons) do
        btn.BackgroundColor3 = (k == tabName) and Color3.fromRGB(44,88,225) or Color3.fromRGB(35,48,85)
    end
end

-- Connect buttons to panel switching
for tabName, btn in pairs(TabButtons) do
    btn.MouseButton1Click:Connect(function()
        showPanel(tabName)
    end)
end

-- Show the default/start tab (Autobuy or any tab you prefer)
showPanel("Autobuy")
--[[
================================================================================
BLOCK 2.5: AMBIENT NEON GLOW PANEL SHELL (Animated background aura)
================================================================================
]]
-- Place immediately after the main 'frame' is created in Block 2:
local neonFrame = Instance.new("Frame")
neonFrame.Size = UDim2.new(1,0,1,0)
neonFrame.Position = UDim2.new(0,0,0,0)
neonFrame.BackgroundTransparency = 0.81
neonFrame.BackgroundColor3 = Color3.fromRGB(22,255,220)
neonFrame.ZIndex = 0
neonFrame.Parent = frame

local neonGrad = Instance.new("UIGradient", neonFrame)
neonGrad.Color = ColorSequence.new{
   ColorSequenceKeypoint.new(0, Color3.fromRGB(0,255,180)),
   ColorSequenceKeypoint.new(0.4, Color3.fromRGB(38,158,255)),
   ColorSequenceKeypoint.new(0.8, Color3.fromRGB(60,230,255)),
   ColorSequenceKeypoint.new(1, Color3.fromRGB(44,252,207))
}
neonGrad.Transparency = NumberSequence.new{
   NumberSequenceKeypoint.new(0,0.08),
   NumberSequenceKeypoint.new(0.4,0.23),
   NumberSequenceKeypoint.new(1,0.85)
}
-- Animate the offset to create a moving glow effect
RunService.RenderStepped:Connect(function()
   neonGrad.Offset = Vector2.new(math.sin(tick()*0.23)*0.11, math.cos(tick()*0.22)*0.14)
end)
--[[
================================================================================
BLOCK 2.7: ANIMATED BUTTON HOVER (Scale & Color Tween for Tab/Main Buttons)
================================================================================
]]
-- After your tabButtons are all created in Block 2, run this for each:
local TweenService = game:GetService("TweenService")

for _,btn in ipairs(tabButtons) do
    -- Add a UIGradient for neon shimmer on buttons
    local grad = Instance.new("UIGradient", btn)
    grad.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0,Color3.fromRGB(0,255,180)),
        ColorSequenceKeypoint.new(0.5,Color3.fromRGB(60,200,245)),
        ColorSequenceKeypoint.new(1,Color3.fromRGB(0,160,255))
    }
    grad.Rotation = 90

    -- Animate gradient shimmer (for scifi shine)
    RunService.RenderStepped:Connect(function()
        grad.Offset = Vector2.new(math.sin(tick()*0.88+btn.Position.X.Offset/100)*0.16, 0)
    end)

    -- Hover: scale up, highlight color, then revert smoothly
    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.16, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
            Size = btn.Size + UDim2.new(0, 10, 0, 8),
            BackgroundColor3 = Color3.fromRGB(0, 255, 210)
        }):Play()
    end)
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.21, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 168, 0, 48),
            BackgroundColor3 = Color3.fromRGB(24,24,38)
        }):Play()
    end)
end

-- Example: For a main big action button (like "Autobuy" ON/OFF), you can use the same pattern:
-- TweenService:Create(abBtn, TweenInfo.new(...), {Size=...,...}):Play() on MouseEnter/MouseLeave (see above).

--[[
================================================================================
BLOCK 3: EMOTES TAB PANEL CONTENT & KEYBINDS (JARVIS SUITE STYLE)
================================================================================
]]

local EmotesPanel = TabPanels["Emotes"]

-- Title
local emoteTitle = Instance.new("TextLabel")
emoteTitle.Text = "Emote Macros"
emoteTitle.Font = Enum.Font.FredokaOne
emoteTitle.TextSize = 28
emoteTitle.TextColor3 = Color3.fromRGB(150, 210, 255)
emoteTitle.BackgroundTransparency = 1
emoteTitle.Size = UDim2.new(1, 0, 0, 38)
emoteTitle.Position = UDim2.new(0, 0, 0, 8)
emoteTitle.Parent = EmotesPanel

-- Macro config: add as many as you want
local EmoteMacros = {
    {
        text = "/e [BEST] Jason Vorhees Shake - Dwerk (Don't Hide)",
        btnLabel = "[Z] Jason Vorhees Shake",
        keycode = Enum.KeyCode.Z,
    },
    -- Example for more:
    -- {
    --   text="/e Worm Wiggle",
    --   btnLabel="[X] Worm Wiggle",
    --   keycode=Enum.KeyCode.X,
    -- },
}
local macroBtnHeight, macroSpace = 48, 12
local KeycodeToMacroFn = {}

for i, macro in ipairs(EmoteMacros) do
    local btn = Instance.new("TextButton")
    btn.Text = macro.btnLabel or macro.text
    btn.Font = Enum.Font.FredokaOne
    btn.TextSize = 22
    btn.TextColor3 = Color3.fromRGB(242,242,255)
    btn.BackgroundColor3 = Color3.fromRGB(39,60,130)
    btn.Size = UDim2.new(1, -40, 0, macroBtnHeight)
    btn.Position = UDim2.new(0, 20, 0, 52 + (i-1)*(macroBtnHeight + macroSpace))
    btn.AutoButtonColor = true
    btn.Parent = EmotesPanel

    local function sendEmote()
        local chatEvents = game:GetService("ReplicatedStorage"):FindFirstChild("DefaultChatSystemChatEvents")
        if chatEvents and chatEvents:FindFirstChild("SayMessageRequest") then
            chatEvents.SayMessageRequest:FireServer(macro.text, "All")
        end
    end
    btn.MouseButton1Click:Connect(sendEmote)
    if macro.keycode then
        KeycodeToMacroFn[macro.keycode.Value] = sendEmote
    end
end

-- Keybinds â€“ only fire if EMOTES tab is visible
game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if not EmotesPanel.Visible or gameProcessed then return end
    local fn = KeycodeToMacroFn[input.KeyCode.Value]
    if fn then fn() end
end)

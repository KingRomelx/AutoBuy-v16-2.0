reEquipGui.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")

--================== END MODAL RE-EQUIP GUI BLOCK ==================--

local lastSlotState = {nil,nil,nil}
local promptOpen = {false, false, false}
local pendingPrompt = {nil, nil, nil}

game:GetService("RunService").RenderStepped:Connect(function()
    if not pan.Visible then return end
    for i = 1,3 do
        local ds = myInfo:FindFirstChild("DisplaySneaker"..i)
        local statusText, col, emoji = "EMPTY", GRAY, "âšª"
        --===* If there's a pending prompt and slot is now empty, show prompt! *===--
        if not ds or ds.Value == "" then
            if pendingPrompt[i] and not promptOpen[i] then
                promptOpen[i] = true -- block all reentry until closed
                local slotName, removedName, mp = unpack(pendingPrompt[i])
                coroutine.wrap(function()
                    reEquipGui.Enabled = true
                    ShoeLabel.Text = ("Shoe: %s\nSlot: %s"):format(removedName, slotName)
                    PriceBox.Text = tostring(mp or "")
                    ConfirmBox.Text = ""
                    local inputLock = true
                    local conn1, conn2, conn3

                    local function validateConfirm()
                        -- Rome check
                        local allowed = (ConfirmBox.Text == "Rome")
                        Confirm.BackgroundColor3 = allowed and Color3.fromRGB(80,200,120) or Color3.fromRGB(100,110,100)
                        return allowed
                    end
                    ConfirmBox:GetPropertyChangedSignal("Text"):Connect(validateConfirm)
                    validateConfirm()

                    local function closePrompt()
                        reEquipGui.Enabled = false
                        inputLock = false
                        if conn1 then conn1:Disconnect() end
                        if conn2 then conn2:Disconnect() end
                        if conn3 then conn3:Disconnect() end
                        promptOpen[i] = false
                        pendingPrompt[i] = nil
                    end
                    conn1 = Confirm.MouseButton1Click:Connect(function()
                        local v = tonumber(PriceBox.Text)
                        if (ConfirmBox.Text ~= "Rome") then
                            ConfirmBox.Text = ""
                            ConfirmBox.PlaceholderText = "MUST enter Rome"
                            validateConfirm()
                            return
                        end
                        if not (v and v > 0) then
                            PriceBox.Text = ""
                            PriceBox.PlaceholderText = "Enter valid price..."
                            return
                        end
                        closePrompt()
                        local args = {removedName, slotName, v}
                        remote:FireServer(unpack(args))
                        game:GetService("StarterGui"):SetCore("SendNotification", {
                            Title = "Re-equipped!",
                            Text = ("%s added back for $%s"):format(removedName, v),
                            Duration = 3,
                        })
                    end)
                    conn2 = Cancel.MouseButton1Click:Connect(function()
                        closePrompt()
                    end)
                    conn3 = UIS.InputBegan:Connect(function(input, gP)
                        if not inputLock or gP then return end
                        if input.KeyCode == Enum.KeyCode.Return then
                            Confirm.MouseButton1Click:Fire()
                        elseif input.KeyCode == Enum.KeyCode.Escape then
                            Cancel.MouseButton1Click:Fire()
                        end
                    end)
                    while inputLock do wait() end
                end)()
            end
            lastSlotState[i] = nil
        else
            statusText = ("%s ($%s)"):format(ds.Value, tostring(ds:GetAttribute("Price") or "??"))
            local matching = nil
            for _,shoe in ipairs(SM_SHOES) do
                if ds.Value == shoe.name then
                    matching = shoe
                    break
                end
            end
            if matching then
                local price = ds:GetAttribute("Price") or 0
                if tonumber(price) and tonumber(price) > matching.max_price then
                    col = GREEN; emoji = "ðŸŸ¢"
                    if enableSMNotifs and lastSlotState[i] ~= "GREEN_"..matching.name then
                        game:GetService("StarterGui"):SetCore("SendNotification", {
                            Title = "ðŸŸ¢ GREEN!",
                            Text = ("%s in slot %d is GOOD TO GO!"):format(matching.name, i),
                            Duration = 4,
                        })
                        lastSlotState[i] = "GREEN_"..matching.name
                    end
                    promptOpen[i] = false
                    pendingPrompt[i] = nil
                elseif tonumber(price) and tonumber(price) <= matching.max_price then
                    col = RED; emoji = "ðŸ”´"
                    -- If not already pending, remove and set pending!
                    if enableSMNotifs and lastSlotState[i] ~= "RED_"..matching.name and not promptOpen[i] and not pendingPrompt[i] then
                        lastSlotState[i] = "RED_"..matching.name
                        game:GetService("StarterGui"):SetCore("SendNotification", {
                            Title = "ðŸ”´ RED!",
                            Text = ("%s in slot %d is UNDER price - REMOVED!"):format(matching.name, i),
                            Duration = 4,
                        })
                        local slotName = "DisplaySneaker"..i
                        local removedName = ds.Value
                        remote:FireServer("", slotName, 0)
                        pendingPrompt[i] = {slotName, removedName, matching.max_price}
                        promptOpen[i] = false
                    end
                end
            else
                lastSlotState[i] = nil
                promptOpen[i] = false
                pendingPrompt[i] = nil
            end
        end
        smStatusLabels[i].Text = ("%s Slot %d: %s"):format(emoji, i, statusText)
        smStatusLabels[i].BackgroundColor3 = col:Lerp(Color3.new(0,0,0),0.84)
    end
end)
--[[
================================================================================
BLOCK 4: AUTOBUY LOGIC (NO "MainPart" check!)
================================================================================
]]

local recentBuys = {}

function showAutoBuyPopup(msg)
    print("[AUTOBUY]", msg)
    announce("[AUTOBUY] " .. msg)
end

RunService.Heartbeat:Connect(function()
    if not (autoBuy or oneDollarSnipe) then return end
    local tablesFolder = Workspace:FindFirstChild("Tables")
    if not tablesFolder or not buyDisplayRemote then return end
    for _,tbl in ipairs(tablesFolder:GetChildren())do
        for i=1,3 do
            local display = tbl:FindFirstChild("DisplaySneaker"..i)
            -- ONLY these 2 checks:
            if display and display:FindFirstChild("SneakerValue") and display:FindFirstChild("Price") then
                local sneakerValue = tostring(display.SneakerValue.Value)
                local price = tonumber(display.Price.Value)
                local buyKey = sneakerValue..":"..tostring(price)
                if recentBuys[buyKey] and tick()-recentBuys[buyKey]<1 then
                    continue
                end
                if isBlacklisted(sneakerValue) then continue end

                if oneDollarSnipe and price == 1 then
                    buyDisplayRemote:FireServer(display, sneakerValue, price)
                    showAutoBuyPopup("[$1 SNIPE]\n" .. sneakerValue .. " @ $1\nTime: " .. os.date("%I:%M:%S %p"))
                    logPurchase(sneakerValue, price, os.date("%I:%M:%S %p"))
                    recentBuys[buyKey] = tick()
                    --=== UPDATED: Logging this sale with username ===--
                    table.insert(serverSalesLog, {
                        time = os.time(),
                        user = game:GetService("Players").LocalPlayer.Name,
                        shoe = sneakerValue,
                        price = price,
                    })
                    continue
                end

                if autoBuy then
                    local allowed,maxPrice=false,nil
                    for _,preset in ipairs(presetSneakers) do
                        if sneakerValue:lower() == preset.full:lower() or sneakerValue:lower() == preset.label:lower() then
                            maxPrice = shoeMaxPrices[preset.full] or preset.default
                            allowed = true
                            break
                        end
                    end
                    if allowed and maxPrice and price <= maxPrice then
                        buyDisplayRemote:FireServer(display, sneakerValue, price)
                        showAutoBuyPopup("[AUTOBUY]\n" .. sneakerValue .. " @ $" .. formatNumber(price) .. "\nTime: " .. os.date("%I:%M:%S %p"))
                        logPurchase(sneakerValue, price, os.date("%I:%M:%S %p"))
                        recentBuys[buyKey] = tick()
                        --=== UPDATED: Logging this sale with username ===--
                        table.insert(serverSalesLog, {
                            time = os.time(),
                            user = game:GetService("Players").LocalPlayer.Name,
                            shoe = sneakerValue,
                            price = price,
                        })
                    end
                end
            end
        end
    end
end)

    PlayAlert = not PlayAlert
    alertToggleBtn.Text = "Ding Alert: " .. (PlayAlert and "ON" or "OFF")
    alertToggleBtn.BackgroundColor3 = PlayAlert and Color3.fromRGB(230,255,120) or Color3.fromRGB(120,130,140)
end)

